        <Key android:codes="9982" android:keyWidth="7%p " android:keyLabel="fast"/>

        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
                android:keyWidth="17%p" android:keyEdgeFlags="right"
                />
    </Row>
    
    <Row>
        <Key android:codes="9988" android:keyWidth="6%p " android:keyLabel="cover" android:horizontalGap="30%p"/>
        <Key android:codes="9987" android:keyWidth="7%p " android:keyLabel="back"/>
